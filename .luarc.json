{
    "runtime.version": "LuaJIT",
    "diagnostics": {
        "enable": true,
        "globals": [
          "vim",
          "describe",
          "pending",
          "it",
          "before_each",
          "after_each",
          "setup",
          "teardown",
          "finally",
          "lfs"
        ]
    },
    "workspace": {
        "library": [
            "/usr/share/nvim/runtime/lua/"
        ],
        "checkThirdParty": false,
        "maxPreload": 2000,
        "preloadFileSize": 1000
    },
    "telemetry.enable": false
}
